{
  "name": "t5c",
  "version": "0.1.9",
  "repository": {
    "url": "git+https://github.com/oriongunning/t5c.git"
  },
  "description": "Building a basic multiplayer 3d top down rpg using babylon.js and colyseus",
  "main": "./dist/client/index.html",
  "scripts": {
    "client-dev": "webpack serve --open --config webpack.dev.js",
    "client-start": "webpack serve --open --config webpack.dev.js",
    "client-build": "webpack --config webpack.prod.js",
    "server-dev": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "server-start": "ts-node --transpile-only src/server/index.ts",
    "server-build": "tsc",
    "loadtest": "npx colyseus-loadtest ./src/server/loadtest/test.ts --room game_room --numClients 20 --endpoint ws://localhost:3000"
  },
  "author": "Orion",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babylonjs/gui-editor": "5.53.0",
    "@babylonjs/inspector": "5.53.0",
    "@babylonjs/materials": "5.53.0",
    "@colyseus/core": "0.15.0-preview.6",
    "@colyseus/loadtest": "0.15.0-preview.0",
    "@colyseus/monitor": "^0.15.0-preview.3",
    "@colyseus/ws-transport": "0.15.0-preview.2",
    "@colyseus/command": "0.2.1",
    "@types/node": "^17.0.12",
    "@types/react": "16.7.3",
    "@types/react-dom": "16.0.9",
    "colyseus": "0.15.0-preview.4",
    "copy-webpack-plugin": "^11.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3",
    "webpack": "^5.66.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@babylonjs/core": "5.53.0",
    "@babylonjs/gui": "5.53.0",
    "@babylonjs/loaders": "5.53.0",
    "@babylonjs/serializers": "5.53.0",
    "babylonjs-gltf2interface": "5.53.0",
    "colyseus.js": "^0.15.0-preview.5",
    
    "cors": "^2.8.5",
    "dotenv-webpack": "^7.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "nanoid": "^3.0.0",
    "sqlite3": "^5.1.2",
    "winston": "^3.8.2"
  },
  "peerDependencies": {},
  "browserslist": {
    "production": [
      "> 5%"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
