{
  "private": true,
  "name": "my-app",
  "version": "1.0.0",
  "description": "npm init template for bootstrapping an empty Colyseus project",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "start": "ts-node --transpile-only src/index.ts",
    "loadtest": "colyseus-loadtest loadtest/example.ts --room my_room --numClients 2",
    "build": "tsc",
    "clean": "node node_modules/rimraf/bin dist",
    "test": "mocha --require ts-node/register test/**_test.ts --exit --timeout 15000"
  },
  "author": "",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/colyseus/create-colyseus/issues"
  },
  "homepage": "https://github.com/colyseus/create-colyseus#readme",
  "devDependencies": {
    "colyseus.js": "^0.15.0-preview.0",
    "@colyseus/loadtest": "0.15.0-preview.0",
    "@types/cors": "^2.8.12",
    "@types/node": "^18.11.5",
    "@types/express": "^4.17.14",
    "@types/mocha": "^10.0.0",
    "copyfiles": "^2.4.1",
    "mocha": "^9.0.2",
    "rimraf": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@colyseus/arena": "0.15.0-preview.0",
    "@colyseus/monitor": "0.15.0-preview.0",
    "colyseus": "0.15.0-preview.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "winston": "^3.8.2"
  }
}
